using System;

namespace practica1
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Ingresar Nombre de Usuario");
            String s = Console.ReadLine();
            if(s == " "){
                Console.WriteLine("Hola Mundo");
            }
            else {
                Console.WriteLine("Bienvenido: "+s+" a este equipo");
            }
            Console.ReadKey();
        }
    }
}
