static void Main(string[] args)
        {
            char c;
            char? c2;
            string? st;
            c = "";
            c = '';
            c = null;
            c2 = null;
            c2 = (65 as char?);
            st = "";
            st = '';
            st = null;
            st = (char)65;
            st = (string)65;
            st = 47.89.ToString();
            Console.ReadKey();
        }
    }
    
//PROBLEMAS EN COMPILADOR//

ERROR LINEA4> SE LE PASA UN STRING VACIO A UNA VARIABLE CHAR SIN CONVERSION.
ERROR LINEA6> SE LE PASA UN VALOR NULO A UNA VARIABLE QUE NO PUEDE ACEPTAR VALORES NULOS
ERROR LINEA12> SE LE PASA UN CHAR A UN STRING
ERRIR LINEA13 Y LINEA14> HACE UNA CONVERSION ERRONEA, DEBERIA SER UN .TOSTRING
